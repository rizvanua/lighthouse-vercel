{
    "version": 2,
    "buildCommand": "cd api && npm install && npm run build",
    "outputDirectory": "api/dist",
    "functions": {
      "api/**/*.ts": {
        "memory": 1024,
        "maxDuration": 60
      }
    },
    "routes": [      
      {
        "src": "/api/analyze",
        "dest": "/api/analyze.ts"
      }
    ]
  }
